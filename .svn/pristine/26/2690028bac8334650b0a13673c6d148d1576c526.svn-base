h1 {
	color: #004c75;
	margin: 0;
	padding: 0;
	font: normal 2.4em/1em Arial, Helvetica, sans-serif;
}

h2 {
	font-size: 0.9em;
	line-height: 1.25em;
	color: #0270de;
	font-weight: bold;
	margin: 0;
	padding: 0;
}

.services {
	padding-bottom: 2px;
	border-bottom-width: thin;
	border-bottom-style: none;
	border-bottom-color: #dcdcdc;
}

.empty_bottom {
	padding-bottom: 20px;
}

.row_services { 
	*height: 1%;
	overflow: hidden;
	padding: 14px 0 4px 0;
}

.service {
	width: 22%;
	margin-right: 1%;
	float: left;
}

.service p {
	font-size: 0.9em;
	line-height: 1.25em;
	color: #333;
	padding: 0;
}

.service h2 a {
	text-decoration: none;
	color: #0270de;
}

h1 {
	margin: 0;
	padding: 0 0 5px 0;
	color: #004c75;
	font: bold 1.25em Arial, Helvetica, sans-serif;
}

.description_page {
	padding: 6px 0 3px 0;
}

.description_page .services_img .shadowed_img img {
	padding: 5px;
	border-color: #cfdef0;
}

.description_page p {
	color: #333;
	padding: 5px 0 0 0;
	line-height: 1.4em;
}

.description_page p.first {
	padding: 0;
}

.description_page ul {
	margin: 0;
	padding: 3px 0 0 15px;
	list-style: none;
}

.description_page ul li {
	margin: 0;
	padding: 0 0 0 8px;
	background: url({{ MEDIA_URL }}/images/4x4_bullet_page_description.gif) left 8px no-repeat;
	color: #656769;
	line-height: 1.85em;
}

.cols_2_fix_right_160_sub {
	margin: 0;
	padding: 8px 60px 0 0; 
	*height: 1%;
	overflow: hidden;
}

.cols_2_fix_right_160_sub .cols_2_left_sub {
	width: 100%;
	margin-left: 20px;
	margin-right: -160px;
	float: left;
}

.cols_2_fix_right_160_sub .cols_2_right_sub {
	width: 160px;
	margin-right: -60px;
	margin-left: 20px;
	float: left;
}

.cols_2_fix_right_160_sub .cols_2_left_sub_sub {
	padding-right: 11px;
}

form.go_form {
	padding: 0 0 18px 0;
}

.go_form .wrap_select {
	padding: 7px 0 0 2px;
}

.go_form .submit {
	padding: 23px 4px 0 0;
}

.go_form input {
	background: url({{ MEDIA_URL }}/images/69x31_go_button.gif);
	width: 69px;
	height: 31px;
}

.bottomlinks {
	padding: 17px 0 0 8px;
}

ul {
	list-style: none;
	margin: 0;
	padding: 0;
}

ul li {
	margin: 0;
	padding: 5px 0 0 10px;
	line-height: 1.25em;
	background: url({{ MEDIA_URL }}/images/6x6_bullet_add_services.gif) left 10px no-repeat;
	color: #656769;
}

ul li a {
	color: #656769;
	text-decoration: none;
}

ul li a:hover {
	color: #0047a8;
}

p {
	color: #333;
	padding: 5px 0 0 0;
	line-height: 1.4em;
}

.cols_2_left_sub_sub h2 {
	font: bold 1.75em/ 1em Arial, Helvetica, sans-serif;
	color: #c41230;
	margin: 0;
	padding: 0;
}

.cols_2_left_sub_sub h3 {
	font: bold 1.25em/ 1em Arial, Helvetica, sans-serif;
	color: #c41230;
	margin: 0 0 0 0;
	padding: 12px 0 0 0;
	line-height: 0;
	margin-bottom: 0;
}