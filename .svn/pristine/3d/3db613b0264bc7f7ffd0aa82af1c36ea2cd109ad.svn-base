/* Custom CSS - Daniel Rothier */
.r_image_thumbnail { float:left;  }
.r_image_thumbnail img {  }
.r_clearfix { float: none; clear: both; }
.r_doctor_name { float: left; display: inline; margin-top: 8px !important; width: 520px; color: #000 !important; border-bottom: 1px dotted gainsboro; padding-bottom: 6px !important; }
.r_doctor_details { float: left; display: inline; width: 520px; }
.r_blue_title { color: #0047BB !important; margin-bottom: 2px !important; margin-top: 0px	}
.r_icon_success { float: left; display: inline; font-size:24px; color: green; width: 14px; margin-top: -1px; height: 15px; margin-right: 4px; }
.r_icon_error { float: left; display: inline; font-size:24px; color: red; width: 14px; height: 15px; margin-top: -1px; margin-right: 4px; }
.r_text_success { font-weight: bold !important; !important; width: 140px; margin-top: 8px !important; display: block; }
.r_text_error { font-weight: bold !important; width: 140px; margin-top: 16px !important; display: block; }
.r_left_col { float: left; display: inline; width: 200px; margin-right: 30px; }
.large_td { width: 362px; border-right: 1px dotted gainsboro; border-bottom: 1px dotted gainsboro; vertical-align: top; }
.small_td { width: 158px;  padding-left: 5px; padding-bottom: 0px; border-bottom: 1px dotted gainsboro; vertical-align: top;}
.r_doctor_extra { float: left; display: inline; clear: both; width: 682px; }
.r_doctor_extra tr { float: left; display: inline; width: 304px; margin-right: 34px;}
.r_doctor_extra td { float: left; display: inline; width: 304px; padding: 1px; }
p.description { margin-top: 0px !important; width: 304px; }
.r_doctor_about { float: left; display: inline; clear: both; position: relative; margin-top: 40px; width: 688px; min-height: 60px; background: url("{{ MEDIA_URL }}images/info_bg.gif") repeat-y; }
.r_doctor_about_header { position: absolute; left: 0px; top: -26px; width: 688px; height: 26px; background: url("{{ MEDIA_URL }}images/info_header.gif") no-repeat; }
.r_doctor_about_footer { position: absolute; left: 0px; bottom: -49px; width: 688px; height: 49px; background: url("{{ MEDIA_URL }}images/info_footer.gif") 0px 0px no-repeat; }
.r_doctor_about p { width: 650px; margin-left: 20px; font-size: 13px; line-height: 16px; }
.r_specialize { float: left; display: inline; clear: both; margin-top: 10px; width: 681px; }
.first_tr { clear: both; }