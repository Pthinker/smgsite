	.doctor_photo_flyover {
		display: none;
	}
	#alphabet_nav_box {
		display: none;
	}
	div.back_top {
		visibility: hidden;
	}
	#patient-tools-box {
		display: none;
	}
	#search-results {
		display: none;
	}
	.right_main_block {
