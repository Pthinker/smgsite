/* Share widget */
ul.share-container {
	margin: 0;
	padding: 0;
	float: right;
}

ul.share-container li {
	margin: 0;
	padding: 0;
	list-style-type: none;
	background: none;
}

ul.share-container li img {
	margin: 0 0 0 14px;
}

ul.share-container li ul {
	position: absolute;
	left: 0;
	top: 14px;
	width: 270px;
	display: none;
	z-index: 9999;
	padding: 0;
	margin: 0;
	border: 1px solid #dfe1e1;
	background: #fff url( {{ MEDIA_URL }}images/1x30_gradient_blue.gif ) left top repeat-x;
}

#share-widget li {
	text-align: left;
	padding: 5px 5px 10px 5px;
}

#share-widget li a {
	background: url( http://s7.addthis.com/static/r05/widget05.gif ) left center no-repeat;
	display: block;
	float: left;
	line-height: 16px;
	height: 16px;
	margin: 4px 0 4px 0;
	width: 48%;
}

#share-widget li a span {
	padding: 0 0 0 20px;
}

#share-widget li a.share-email {
	background-position: 0 -192px;
}

#share-widget a.share-digg {
	background-position: 0 -160px;
}

#share-widget a.share-reddit {
	background-position: 0 -592px;
}

#share-widget a.share-linkedin {
	background-position: 0 -384px;
}

#share-widget a.share-facebook {
	background-position: 0 -208px;
}

#share-widget a.share-delicious {
	background-position: 0 -144px;
}

#share-widget a.share-stumbleupon {
	background-position: 0 -704px;
}

#share-widget a.share-twitter {
	background-position: 0 -784px;
}

